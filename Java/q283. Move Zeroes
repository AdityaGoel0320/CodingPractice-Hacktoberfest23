class Solution {
    public void moveZeroes(int[] arr) {
        int l = arr.length ; 
        int[] ans  = new int[l] ;
        int k = 0; 
        for(int i = 0 ; i<=l-1 ; i++){
            if(arr[i]!=0){
                ans[k++] = arr[i] ; 

            }
           
        }
        System.out.println(Arrays.toString(ans)) ; 
          for(int i = 0 ; i<=l-1 ; i++){
            arr[i] = ans[i] ; 
           
        }
    }
}